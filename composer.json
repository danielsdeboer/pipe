{
    "name": "aviator/pipe",
    "type": "library",
    "description": "Chain callables using a simple wrapper class.",
    "keywords": [
        "php",
        "php7",
        "pipe"
    ],
    "homepage": "https://github.com/danielsdeboer/pipe",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel S. Deboer",
            "email": "daniel.s.deboer@gmail.com",
            "homepage": "https://aviatorcreative.ca",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aviator\\Pipe\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.4.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Aviator\\Pipe\\Tests\\": "tests/"
        },
        "files": ["dev/functions.php"]
    },
    "scripts": {
        "test": "phpunit"
    }
}
